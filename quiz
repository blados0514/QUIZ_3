--QUIZ BLAIMIR OSPINA

CREATE TABLESPACE quiz_manana DATAFILE
'quiz_manana_1' SIZE 30M,
'quiz_manana_2' SIZE 30M,
'quiz_manana_3' SIZE 30M;

--CREATE ROL
CREATE PROFILE estudiante_quiz LIMIT
IDLE_TIME 15;

--USER_1
CREATE USER carlos_2 IDENTIFIED BY carlos_2 DEFAULT TABLESPACE  quiz_manana QUOTA UNLIMITED ON  quiz_manana;

GRANT CONNECT TO carlos_2;

GRANT estudiante_quiz TO carlos_2;   

GRANT CREATE TABLE TO carlos_2;

--USER 2
CREATE USER user2 IDENTIFIED BY user2 DEFAULT TABLESPACE  quiz_manana QUOTA UNLIMITED ON  quiz_manana;  

GRANT CONNECT TO carlos_2;

GRANT estudiante_quiz TO carlos_2;

--QUERYS 1

SELECT
  GENERO,
  FECHA_NACIMIENTO,
  CONSECUTIVO,
  MUNICIPIO_RESIDENCIA,
  PUNTAJE_GLOBAL
FROM ICFES 
WHERE FECHA_NACIMIENTO = (SELECT MIN(FECHA_NACIMIENTO) FROM ICFES);

--QUERYS 2 

SELECT
  GENERO,
  FECHA_NACIMIENTO,
  CONSECUTIVO,
  MUNICIPIO_RESIDENCIA,
  PUNTAJE_GLOBAL
FROM ICFES 
WHERE DESEMPENO_INGLES = 'B1';

--QUERYS 3 

SELECT COUNT(DISTINCT COLEGIO_NOMBRE) FROM ICFES; 

--QUERYS 4 

SELECT COUNT( TIPO_DOCUMENTO) FROM ICFES
WHERE MUNICIPIO_RESIDENCIA = 'PASTO';

 

